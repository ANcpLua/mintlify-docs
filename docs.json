{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "ANcpLua Framework",
  "colors": {
    "primary": "#0E7490",
    "light": "#0891B2",
    "dark": "#0A6A80"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "metadata": {
    "og:site_name": "ANcpLua Framework Documentation",
    "og:image": "/images/og-image.png",
    "og:type": "website",
    "og:locale": "en_US",
    "twitter:card": "summary_large_image"
  },
  "search": {
    "prompt": "Search SDK, analyzers, utilities..."
  },
  "seo": {
    "indexing": "navigable"
  },
  "api": {
    "playground": {
      "mode": "show"
    },
    "baseUrl": ["https://api.qyl.dev", "https://api.staging.qyl.dev"]
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["index", "quickstart"]
          },
          {
            "group": "ANcpLua.NET.Sdk",
            "pages": [
              "sdk/overview",
              "sdk/variants",
              "sdk/msbuild-properties",
              "sdk/service-defaults",
              "sdk/polyfills",
              "sdk/extensions",
              "sdk/shared-utilities",
              "sdk/banned-apis",
              "sdk/configuration-files",
              "sdk/testing"
            ]
          },
          {
            "group": "ANcpLua.Analyzers",
            "pages": [
              "analyzers/overview",
              "analyzers/configuration",
              {
                "group": "Rules",
                "pages": [
                  "analyzers/rules/AL0001",
                  "analyzers/rules/AL0002",
                  "analyzers/rules/AL0003",
                  "analyzers/rules/AL0004",
                  "analyzers/rules/AL0005",
                  "analyzers/rules/AL0006",
                  "analyzers/rules/AL0007",
                  "analyzers/rules/AL0008",
                  "analyzers/rules/AL0009",
                  "analyzers/rules/AL0010",
                  "analyzers/rules/AL0011",
                  "analyzers/rules/AL0012",
                  "analyzers/rules/AL0013",
                  "analyzers/rules/AL0014",
                  "analyzers/rules/AL0015",
                  "analyzers/rules/AL0016",
                  "analyzers/rules/AL0017",
                  "analyzers/rules/AL0020",
                  "analyzers/rules/AL0021",
                  "analyzers/rules/AL0022",
                  "analyzers/rules/AL0023",
                  "analyzers/rules/AL0024",
                  "analyzers/rules/AL0025",
                  "analyzers/rules/AR0001",
                  "analyzers/rules/AR0002"
                ]
              }
            ]
          },
          {
            "group": "ANcpLua.Roslyn.Utilities",
            "pages": [
              "utilities/overview",
              "utilities/diagnostic-flow",
              "utilities/pipeline",
              "utilities/contexts",
              "utilities/patterns",
              "utilities/operations",
              "utilities/symbols",
              "utilities/codegen",
              "utilities/semantic-guard",
              "utilities/msbuild-properties",
              "utilities/msbuild-testing",
              "utilities/testing"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "qyl API",
            "pages": ["api-reference/introduction"]
          }
        ],
        "openapi": "/api-reference/openapi.yaml"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "NuGet",
          "href": "https://www.nuget.org/profiles/ANcpLua",
          "icon": "box"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/ANcpLua",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "NuGet",
        "href": "https://www.nuget.org/profiles/ANcpLua"
      },
      {
        "label": "GitHub",
        "href": "https://github.com/ANcpLua"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/ANcpLua"
    }
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "Page Not Found",
      "description": "The page you're looking for doesn't exist or has been moved. Try the [documentation home](/index) to get started, or use the **search bar** above to find what you need."
    }
  },
  "redirects": [
    {
      "source": "/introduction",
      "destination": "/index"
    },
    {
      "source": "/getting-started",
      "destination": "/quickstart"
    },
    {
      "source": "/sdk",
      "destination": "/sdk/overview"
    },
    {
      "source": "/analyzers",
      "destination": "/analyzers/overview"
    },
    {
      "source": "/utilities",
      "destination": "/utilities/overview"
    },
    {
      "source": "/rules/:slug*",
      "destination": "/analyzers/rules/:slug*"
    },
    {
      "source": "/analyzer/:slug*",
      "destination": "/analyzers/:slug*"
    },
    {
      "source": "/api",
      "destination": "/api-reference/introduction"
    },
    {
      "source": "/api-reference",
      "destination": "/api-reference/introduction"
    },
    {
      "source": "/roslyn",
      "destination": "/utilities/overview"
    },
    {
      "source": "/roslyn/:slug*",
      "destination": "/utilities/:slug*"
    }
  ]
}
