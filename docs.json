    "404": {
      "redirect": false,
      "title": "Page Not Found",
      "description": "The page you're looking for doesn't exist or has been moved. Try the [documentation home](/index) to get started, or use the **search bar** above to find what you need."
    }
  },
  "redirects": [
    {
      "source": "/introduction",
      "destination": "/index"
    },
    {
      "source": "/getting-started",
      "destination": "/quickstart"
    },
    {
      "source": "/sdk",
      "destination": "/sdk/overview"
    },
    {
      "source": "/analyzers",
      "destination": "/analyzers/overview"
    },
    {
      "source": "/utilities",
      "destination": "/utilities/overview"
    },
    {
      "source": "/rules/:slug*",
      "destination": "/analyzers/rules/:slug*"
    },
    {
      "source": "/analyzer/:slug*",
      "destination": "/analyzers/:slug*"
    },
    {
      "source": "/api",
      "destination": "/api-reference/introduction"
    },
    {
      "source": "/api-reference",
      "destination": "/api-reference/introduction"
    },
    {
      "source": "/roslyn",
      "destination": "/utilities/overview"
    },
    {
      "source": "/roslyn/:slug*",
      "destination": "/utilities/:slug*"
    },
    {
      "source": "/erroror",
